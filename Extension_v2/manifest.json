{
  "name": "DeFake Video Scanner",
  "description": "Scans Videos for Deep Fakes",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["alarms", "notifications", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "DeFake",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "128": "icon_128.png"
  }
}
